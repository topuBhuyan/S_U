Router(config-if)#exit

Router(config)#ip dhcp excluded-address 192.168.10.1
Router(config)#ip dhcp excluded-address 192.168.30.1

Router(config)#ip dhcp pool 192.168.10.1
Router(dhcp-config)#network 192.168.10.0 255.255.255.0
Router(dhcp-config)#default-router 192.168.10.1

Router(dhcp-config)#ip dhcp pool 192.168.30.1
Router(dhcp-config)#network 192.168.30.0 255.255.255.0
Router(dhcp-config)#default-router 192.168.30.1

Router(dhcp-config)#exit


==========================================
2nd router


Router(config-if)#exit 
Router(config)#ip dhcp excluded-address 192.168.20.1
Router(config)#ip dhcp excluded-address 192.168.30.2

Router(dhcp-config)#ip dhcp pool 192.168.20.1
Router(dhcp-config)#network 192.168.20.0 255.255.255.0
Router(dhcp-config)#default-router 192.168.20.1

Router(dhcp-config)#ip dhcp pool 192.168.30.2
Router(dhcp-config)#network 192.168.30.0 255.255.255.0
Router(dhcp-config)#default-router 192.168.30.2

Router(dhcp-config)#exit
